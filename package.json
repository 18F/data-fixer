{
  "name": "datafixer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "bazel build //...",
    "clean": "bazel clean",
    "coverage": "bazel coverage //... && cd ./bazel-out/darwin-fastbuild/testlogs && genhtml -o coverage datafixer/core/test/coverage.dat && open ./coverage/index.html",
    "format": "yarn run prettier --write datafixer",
    "push": "bazel run //datafixer/context/server:image-push",
    "server": "ibazel run //datafixer/context/server:start",
    "start": "ibazel run //datafixer/context/browser:devserver",
    "storybook": "ibazel run //datafixer/context/storybook:storybook",
    "test": "bazel test //..."
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@bazel/bazelisk": "latest",
    "@bazel/buildifier": "latest",
    "@bazel/ibazel": "latest",
    "@bazel/jasmine": "^2.0.1",
    "@bazel/rollup": "^2.0.1",
    "@bazel/terser": "^2.0.2",
    "@bazel/typescript": "^2.0.1",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@storybook/react": "^6.0.21",
    "@types/jasmine": "^3.5.11",
    "@types/uuid": "^8.3.0",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "husky": "^4.2.5",
    "jasmine": "^3.6.1",
    "jasmine-core": "^3.6.0",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-csso": "^4.0.0",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "terser": "5.3.1",
    "typescript": "^4.0.2"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/node-localstorage": "^1.3.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "express": "^4.17.1",
    "fp-ts": "^2.8.2",
    "fp-ts-routing": "^0.5.4",
    "io-ts": "^2.2.10",
    "io-ts-types": "^0.5.10",
    "node-localstorage": "^2.1.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.7.0",
    "uswds": "^2.8.1",
    "uuid": "^8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  }
}
