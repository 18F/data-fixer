{
  "name": "datafixer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bazel": "DATAFIXER_DATASET_ROOT=file://`pwd`/data dotenv -- bazel",
    "ibazel": "DATAFIXER_DATASET_ROOT=file://`pwd`/data dotenv -- ibazel",
    "build": "bazel build //...",
    "clean": "bazel clean",
    "coverage": "bazel coverage //... && cd ./bazel-out/darwin-fastbuild/testlogs && genhtml -o coverage datafixer/core/test/coverage.dat && open ./coverage/index.html",
    "db:change": "yarn knex migrate:make",
    "db:migrate": "yarn knex migrate:latest",
    "db:rollback": "yarn knex migrate:rollback",
    "db:seed": "yarn knex seed:run",
    "db:version": "yarn knex migrate:currentVersion",
    "format": "yarn run prettier --write datafixer db",
    "knex": "knex --knexfile ./db/knexfile.js",
    "push": "bazel run //datafixer/context/server:image-push",
    "server": "ibazel run //datafixer/context/server:start",
    "start": "ibazel run //datafixer/context/browser:devserver",
    "storybook": "ibazel run //datafixer/context/storybook:storybook",
    "test": "bazel test //..."
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@bazel/bazelisk": "latest",
    "@bazel/buildifier": "latest",
    "@bazel/ibazel": "latest",
    "@bazel/jasmine": "^2.0.1",
    "@bazel/rollup": "^2.0.1",
    "@bazel/terser": "^2.0.2",
    "@bazel/typescript": "^2.0.1",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@storybook/react": "^6.0.21",
    "@types/jasmine": "^3.5.11",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.9",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "husky": "^4.2.5",
    "jasmine": "^3.6.1",
    "jasmine-core": "^3.6.0",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-csso": "^4.0.0",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "terser": "5.3.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "dependencies": {
    "@types/client-sessions": "^0.8.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.6",
    "@types/node-jose": "^1.1.5",
    "@types/node-localstorage": "^1.3.0",
    "@types/openid-client": "^3.7.0",
    "@types/passport": "^1.0.4",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.5",
    "dotenv-cli": "^4.0.0",
    "effector": "^21.3.0",
    "effector-react": "^21.0.6",
    "express": "^4.17.1",
    "fp-ts": "^2.8.2",
    "fp-ts-routing": "^0.5.4",
    "io-ts": "^2.2.10",
    "io-ts-types": "^0.5.10",
    "jwk-to-pem": "^2.0.4",
    "knex": "^0.21.8",
    "morgan": "^1.10.0",
    "node-jose": "^2.0.0",
    "node-localstorage": "^2.1.6",
    "openid-client": "^4.2.1",
    "passport": "^0.4.1",
    "pem-jwk": "^2.0.0",
    "pg": "^8.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.7.0",
    "sqlite3": "^5.0.0",
    "uswds": "^2.8.1",
    "uuid": "^8.3.0",
    "yargs": "^16.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  }
}
